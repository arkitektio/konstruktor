                  "hello": "You cutepy"
                },
                {
                  "type": "admin_user",
                  "hello": "Admin User",
                  "defaults": [{
                    "adminUsername": "admin",
                    "adminPassword": "admin"
                  }]
                },
                {
                  "type": "users",
                  "hello": "Admin User",
                  "defaults": [{
                    "username": "demo",
                    "password": "demo",
